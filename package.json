{
  "name": "compass.js",
  "version": "1.2.3",
  "description": "",
  "main": "./build/Compass.js",
  "types": "./build/Compass.d.ts",
  "scripts": {
    "test": "./node_modules/nyc/bin/nyc.js ./node_modules/.bin/mocha",
    "watch": "webpack --progress --colors --watch",
    "build": "webpack --progress --colors",
    "build-ci": "NODE_ENV=production webpack"
  },
  "keywords": [
    "XMPP",
    "Strophe",
    "Lisa",
    "Compass",
    "Iperity"
  ],
  "author": "Tijs Zwinkels <tijs.zwinkels@iperity.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Iperity/Compass.js.git"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "loglevel": "^1.6.4"
  },
  "files": [
    "build/Compass.js",
    "build/Compass.d.ts",
    "README.md"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/expect": "^1.20.4",
    "@types/jquery": "^3.3.31",
    "@types/jsdom": "^12.2.4",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "@types/strophe": "^1.2.30",
    "awesome-typescript-loader": "^4.0.0",
    "babel-core": "^6.26.3",
    "chai": "^4.2.0",
    "dts-bundle-webpack": "1.0.2",
    "jquery": "^3.4.1",
    "jsdom": "^15.1.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rxjs": "^6.5.3",
    "source-map-support": "^0.5.13",
    "strophe.js": "^1.3.4",
    "strophejs-plugin-pubsub": "0.0.2",
    "ts-node": "8.4.1",
    "tslint": "^5.20.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.6.3",
    "webpack": "3.10.0",
    "webpack-rxjs-externals": "^2.0.0"
  }
}
