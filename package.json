{
  "name": "compass.js",
  "version": "1.1.4",
  "description": "",
  "main": "./build/Compass.js",
  "types": "./build/Compass.d.ts",
  "scripts": {
    "test": "./node_modules/nyc/bin/nyc.js ./node_modules/.bin/mocha --compilers ts:ts-node/register ./test/*.ts",
    "watch": "webpack --progress --colors --watch",
    "build": "webpack --progress --colors",
    "build-ci": "NODE_ENV=production webpack"
  },
  "keywords": [
    "XMPP",
    "Strophe",
    "Lisa",
    "Compass",
    "Iperity"
  ],
  "author": "Tijs Zwinkels <tijs.zwinkels@iperity.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Iperity/Compass.js.git"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "loglevel": "^1.6.1"
  },
  "files": [
    "build/Compass.js",
    "build/Compass.d.ts",
    "README.md"
  ],
  "devDependencies": {
    "rxjs": "^6.2.2",
    "strophe.js": "^1.2.16",
    "strophejs-plugin-pubsub": "0.0.2",
    "@types/chai": "^4.1.4",
    "@types/expect": "^1.20.3",
    "@types/jquery": "^3.3.4",
    "@types/jsdom": "^11.0.6",
    "@types/mocha": "^5.2.3",
    "@types/node": "^10.5.0",
    "@types/strophe": "^1.2.30",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel-core": "^6.26.0",
    "chai": "^4.1.2",
    "dts-bundle-webpack": "1.0.0",
    "jquery": "^3.3.1",
    "jsdom": "^11.11.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.9.2",
    "webpack": "3.10.0",
    "webpack-rxjs-externals": "^2.0.0"
  }
}
